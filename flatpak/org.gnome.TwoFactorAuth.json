        {
          "type":"git",
          "url":"https://git.gnome.org/browse/gnome-keyring/",
          "branch":"gnome-3-22"
        }
      ]
    },
    {
      "name":"gnome-twofactorauth",
      "buildsystem":"meson",
      "builddir":true,
      "sources":[
        {
          "type":"git",
          "url":"https://github.com/bil-elmoussaoui/Gnome-TwoFactorAuth",
          "branch":"master"
        }
      ]
    }
